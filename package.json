{
  "name": "mgm",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"npm run start:server\" \"npm run serve\"",
    "start:dev": "concurrently \"npm run start:server:dev\" \"npm run serve\"",
    "serve": "ng serve --proxy-config proxy.config.json",
    "start:server": "npm run serve:api",
    "start:server:dev": "npm run serve:api:dev --watch",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "serve:api": "node server/dist/index.js",
    "serve:api:dev": "nodemon server/index.ts",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "^14.2.1",
    "@angular/animations": "^14.0.0-next.0",
    "@angular/cli": "^14.2.1",
    "@angular/common": "^14.0.0-next.0",
    "@angular/compiler": "^14.0.0-next.0",
    "@angular/compiler-cli": "^14.2.0",
    "@angular/core": "^14.0.0-next.0",
    "@angular/forms": "^14.0.0-next.0",
    "@angular/platform-browser": "^14.0.0-next.0",
    "@angular/platform-browser-dynamic": "^14.0.0-next.0",
    "@angular/router": "^14.0.0-next.0",
    "@syncfusion/ej2-angular-grids": "^20.2.45",
    "@syncfusion/ej2-angular-treegrid": "^20.2.45",
    "@syncfusion/ej2-data": "^20.2.45",
    "rxjs": "~7.5.0",
    "socket.io-client": "^4.5.1",
    "tslib": "^2.3.0",
    "typescript": "^4.8.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@types/jasmine": "~4.0.0",
    "concurrently": "^7.3.0",
    "jasmine-core": "~4.3.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "nodemon": "^2.0.19",
    "ts-node": "^10.9.1"
  },
  "engines": {
    "npm": "8.x",
    "node": "16.x"
  }
}
